{
  "name": "RentNearUni",
  "version": "1.0.0",
  "description": "A comprehensive platform specialized in finding apartments, boarding houses, rooms for rent near Sri Lankan Universities ",
  "main": "index.js",
  "scripts": {
    "install-server": "cd server && npm install",
    "start-server": "cd server && node server.js",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "start-client": "cd client && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "cd server && npm run devStart",
    "client": "cd client && npm start",
    "start": "concurrently \"npm run server\"  \"npm run client\""
  },
